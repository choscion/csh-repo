{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": false,
  "semi": false,
  "useTabs": false,
  "tabWidth": 2,
  "endOfLine": "auto",
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "printWidth": 150,
  "trailingComma": "all",
  "insertPragma": false,
  "quoteProps": "as-needed"
}
